442\u0435\u0440 \u0421\u043c\u0435\u0440\u0447, \u0438 \u043e\u043d \u0443\u0432\u043b\u0435\u043a\u0430\u0435\u0442 \u0432\u0430\u0441 \u0432 \u0447\u0443\u0434\u0435\u0441\u043d\u043e\u0435 \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u0435, \u0438\u0437 \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u0432\u044b \u0432\u0435\u0440\u043d\u0435\u0442\u0435\u0441\u044c \u0441\u043e\u0432\u0441\u0435\u043c \u0434\u0440\u0443\u0433\u0438\u043c \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043c - \u0435\u0441\u043b\u0438 \u0432\u0435\u0440\u043d\u0435\u0442\u0435\u0441\u044c... 7. \u0427\u0430\u043a \u041f\u0430\u043b\u0430\u043d\u0438\u043a \u2014 \"\u041a\u043e\u043b\u044b\u0431\u0435\u043b\u044c\u043d\u0430\u044f\". \u0421\u0438\u043d\u0434\u0440\u043e\u043c \u0432\u043d\u0435\u0437\u0430\u043f\u043d\u043e\u0439 \u0441\u043c\u0435\u0440\u0442\u0438 \u043c\u043b\u0430\u0434\u0435\u043d\u0446\u0435\u0432. \u041e\u0442 \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438 \u0434\u044b\u0445\u0430\u043d\u0438\u044f. \u0412\u043d\u0435\u0448\u043d\u0435 \u0437\u0434\u043e\u0440\u043e\u0432\u044b\u0439 \u043c\u043b\u0430\u0434\u0435\u043d\u0435\u0446, \u0442\u0438\u0445\u043e \u0437\u0430\u0441\u044b\u043f\u0430\u044f, \u0443\u0436\u0435 \u043d\u0438\u043a\u043e\u0433\u0434\u0430 \u043d\u0435 \u043f\u0440\u043e\u0441\u043d\u0435\u0442\u0441\u044f. \u0427\u0438\u0441\u043b\u043e \u0443\u043c\u0435\u0440\u0448\u0438\u0445 \u0440\u0430\u0441\u0442\u0435\u0442. \u0421 \u043a\u0430\u0436\u0434\u044b\u043c \u0433\u043e\u0434\u043e\u043c \u0441\u0435\u043c\u044c \u0442\u044b\u0441\u044f\u0447 \u043c\u043b\u0430\u0434\u0435\u043d\u0446\u0435\u0432 \u0437\u0430\u0441\u044b\u043f\u0430\u044e\u0442 \u0432\u0435\u0447\u043d\u044b\u043c \u0441\u043d\u043e\u043c \u0431\u0435\u0437 \u0432\u0441\u044f\u043a\u043e\u0439 \u043d\u0430 \u0442\u043e \u043f\u0440\u0438\u0447\u0438\u043d\u044b. \u0412\u0440\u0430\u0447\u0438 \u043d\u0430\u0437\u044b\u0432\u0430\u044e\u044